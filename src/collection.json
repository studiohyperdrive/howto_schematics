{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "atom": {
      "description": "Generate a new atom.",
      "factory": "./atom/index#atomSchematic",
      "schema": "./schema.json"
    },
    "molecule": {
      "description": "Generate a new molecule.",
      "factory": "./molecule/index#moleculeSchematic",
      "schema": "./schema.json"
    },
    "organism": {
      "description": "Generate a new organism.",
      "factory": "./organism/index#organismSchematic",
      "schema": "./schema.json"
    },
    "page": {
      "description": "Generate a new page.",
      "factory": "./page/index#pageSchematic",
      "schema": "./schema.json"
    },
    "styleguide": {
      "description": "Set up a new styleguide project.",
      "factory": "./styleguide/index#styleguideSchematic"
    },
    "storybook": {
      "description": "Set up a new storybook project.",
      "factory": "./storybook/index#storybookSchematic"
    }
  }
}
